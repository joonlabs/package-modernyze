{
  "name": "joonlabs/package-modernyze",
  "type": "library",
  "description": "Package that supports the connection to a Modernyze update server.",
  "keywords": [],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0|^9.0",
    "ext-zip": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "Modernyze\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Examples\\": "examples/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Modernyze\\ModernyzeServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit tests"
  }
}
